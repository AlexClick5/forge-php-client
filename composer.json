{
  "name": "autodesk/client",
  "description": "",
  "keywords": [
    "swagger",
    "php",
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/Developer-Autodesk/forge-api-php-client",
  "license": "Apache 2.0",
  "authors": [
    {
      "name": "Developer-Autodesk",
      "homepage": "https://github.com/Developer-Autodesk/forge-api-php-client"
    }
  ],
  "require": {
    "php": ">=5.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "autodesk/core": "*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "satooshi/php-coveralls": "~1.0",
    "squizlabs/php_codesniffer": "~2.6",
    "friendsofphp/php-cs-fixer": "~1.12",
    "symfony/var-dumper": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Autodesk\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Autodesk\\Client\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://git.autodesk.com/Forge-SDKs/php-core-sdk"
    }
  ]
}
